---
import { loadOnlineProgramsGetProgramServiceConfig } from '@wix/headless-online-programs/services';
import OnlineProgramsPage from '../react-pages/online-programs/index';

// Load program by id
const onlineProgramsGetProgramServiceConfig = await loadOnlineProgramsGetProgramServiceConfig({
	programId: '871cbf99-ee47-421f-a155-418d36704380'
})

---

<OnlineProgramsPage
  client:load
	onlineProgramsGetProgramServiceConfig={onlineProgramsGetProgramServiceConfig}
  slot="body"
/>
