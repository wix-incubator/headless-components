{
  "name": "@wix/headless-integrations",
  "packageManager": "yarn@4.4.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -Apv --topological-dev run build",
    "test": "yarn workspaces foreach -Apv --topological-dev run test",
    "lint": "yarn workspaces foreach -Apv --topological-dev run lint"
  },
  "devDependencies": {
    "lefthook": "^1.1.1",
    "lint-staged": "^13.0.3",
    "syncpack": "^8.2.4"
  },
  "resolutions": {

    "@types/react": "^18.2.15"
  },
  "wix": {
    "loki": {
      "enable": true,
      "dedupe": true
    }
  }
}
